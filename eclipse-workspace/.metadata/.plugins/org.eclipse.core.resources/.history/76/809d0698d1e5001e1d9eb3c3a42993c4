package principal;

public class División {

	public static void main(String[] args) {
		int x = 8;
		int y = 15;
		
		if(x > y) {
			double z= (double)x/y;
			System.out.println(z + " E número mayor es " + x);
		}else {
			double z= (double)x/y;
			System.out.println(z  + " El número mayor es " + y );
		}

	}

}
