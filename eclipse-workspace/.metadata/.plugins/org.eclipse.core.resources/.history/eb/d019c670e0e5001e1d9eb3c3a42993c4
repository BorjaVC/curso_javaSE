package principal;

public class Sumador {

	public static void main(String[] args) {
		int n1 = 20;
		int n2 = 11;
		int suma = 0;
		
		for(int i = 1; i<10;i++) {
			if(n1 < 20 && n2 > 11) {
				suma = n1 + n2;
				n1++;
				n2--;
				
			}
		}
		System.out.println(suma);

	}

}
